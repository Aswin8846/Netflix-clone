{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ],
  "extensions": {
    "firestore-stripe-subscriptions-i1pi": "stripe/firestore-stripe-subscriptions@0.1.15",
    "firestore-stripe-subscriptions": "stripe/firestore-stripe-subscriptions@0.1.15"
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
